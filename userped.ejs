<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Sharp:opsz,wght,FILL,GRAD@20..48,100..700,0..1,-50..200" />
    
    <link rel="stylesheet" href="/css/admin.css">


    <title>Pedidos</title>

</head>
<body>
    <div class="container">
        <aside>
            <div class="top">
                <div class="logo">
                    <img src="/images/logo/sw-brown.png">
                    <h2>Shopping <span> World </span></h2>
                </div>
                <div class="close" id="close-btn">
                    <span class="material-symbols-sharp">
                        close
                        </span>
                </div>
            </div>
                <div class="sidebar">
                    <a href="/perfil" >
                        <span class="material-symbols-sharp">
                            group
                            </span>
                            <h3>Perfil</h3>
                    </a>
                    <a href="/userped" class="active">
                        <span class="material-symbols-sharp">
                            list_alt
                            </span>
                            <h3>Pedidos</h3>
                    </a>                
                    <a href="/logout">
                        <span class="material-symbols-sharp">
                            logout
                            </span>
                            <h3>Salir</h3>
                    </a>
                </div>
           
        </aside>
        <!--END OF ASIDE SIDEBAR-->

        <main>
            

            <div class="recent-order">
                <h2>Pedidos</h2>
                <table>
                    <thead>
                        <tr>
                            <th>Cliente</th>
                            <th>Cantidad</th>
                            <th>Productos</th>
                            <th>Talla</th>
                            <th>Color</th>
                            <th>Direcci√≥n</th>
                            <th>Email</th>
                            <th>Numero</th>
                            <th>Monto</th>
                            <th>Link</th>
                            
                        </tr>
                    </thead>
                    <tbody>
                        <% if (data) { %>
                            <% data.forEach(function(item) {%>
                           
                                <tr>
                                    <td><%= item.Nombre%></td>
                                    <td><%= item.cantidad%></td>
                                    <td><%= item.nombreProducto%></td>
                                    <td><%= item.tallaProducto%></td>
                                    <td><%= item.colorProducto%></td>
                                    <td><%= item.direccion%></td>
                                    <td><%= item.correo%></td>
                                    <td><%= item.numero%></td>
                                    <td><%= item.precioU%></td>
                                    <td><%= item.link%></td>

                                </tr>
                                <% })}%>
                    </tbody>
                </table>
              
                
            </div>

        </main>
        <!--end of main-->
        <div class="right">
            <div class="top">
                <button id="menu-btn">
                    <span class="material-symbols-sharp">
                        menu
                        </span>
                </button>
                <div class="theme-toggler">
                    
                    <span class="material-symbols-sharp active">light_mode</span>
                    <span class="material-symbols-sharp">dark_mode</span>
                    
                </div>
                <div class="profile">
                    <div class="info">
                        <p>Hola, <b>marco</b></p>
                    </div>
                    <div class="profile-photo">
                        <img src="https://mejorconsalud.as.com/wp-content/uploads/2017/11/hombre-sonriendo-gafas.jpg" >
                    </div>
                </div>
            </div>
            <!--end of the top-->
           

        </div>
    </div>

    <script src="/js/dashboard-admin.js"></script>
</body>

</html>